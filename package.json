{
  "name": "typeorm-template",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "migrate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run --config config.migrations.ts",
    "migrate:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate --config config.migrations.ts -n",
    "migrate:undo": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert --config config.migrations.ts",
    "start": "ts-node ."
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/pg": "^7.14.7",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "pg": "^8.5.1",
    "typeorm": "^0.2.29"
  }
}
